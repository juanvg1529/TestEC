							"    \r",
							"    // Verificar que el código de estado es 200\r",
							"    pm.response.to.have.status(200);\r",
							"\r",
							"    // Guardar el \"id\" en una variable global\r",
							"    pm.globals.set(\"IdPet\", jsonData.id);\r",
							"\r",
							"    // Obtener el nombre esperado de la variable de entorno\r",
							"    var expectedName = pm.environment.get(\"nameValue\");\r",
							"\r",
							"    // Verificar que el nombre en la respuesta es el mismo que el guardado\r",
							"    pm.test(\"El nombre es el mismo que el guardado\", function () {\r",
							"        pm.expect(jsonData.name).to.eql(expectedName);\r",
